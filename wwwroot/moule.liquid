---
layout: generic.liquid
pagination:
    data: moules
    size: 1
    alias: moule
    reverse: true
permalink: "moules/{{ moule.displayName | slugify }}/"
eleventyComputed:
    title: "{{moule.displayName}} - Anne et ses delices"
---
<ul>
  {% for item in collections.recettes %}
    {% if item.data.recette.mold.name == moule.name and item.data.recette.private!=true %}
      <li>
        <a href="{{item.page.url}}">{{ item.data.recette.title }}</a>
      </li>
    {% endif %}
  {% endfor %}
</ul>